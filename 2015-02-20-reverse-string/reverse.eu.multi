//ORACLE PL/SQL

FUNCTION return_word(word IN VARCHAR2) return VARCHAR

  reversed_string VARCHAR2 := ‘’;

BEGIN
  select reverse(word)
  into reversed_string
  from dual;
  
  RETURN reversed_word;

Exception
when others
then
	DBMS_OUTPUT.PUTLINE(‘Errrrrrm, theres a error!’);
END;


//JAVA

public class CodeShark1{
     public static void main(String args[]) {
       StringBuffer reversed_word = new StringBuffer("Code Shark");
       reversed_word.reverse();
       System.out.println(reversed_word);
   } 
}

